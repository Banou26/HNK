{
  "name": "ozjs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "concurrently": "^3.6.0",
    "standard": "^11.0.1"
  },
  "scripts": {
    "dev": "concurrently \"cargo watch -i dist/* -s \\\"yarn run rust\\\"\" \"yarn run rollup\"",
    "build": "cargo-web build --target=wasm32-unknown-unknown",
    "rollup": "rollup -c -w",
    "rust": "cargo +nightly build --target wasm32-unknown-unknown && wasm-bindgen target/wasm32-unknown-unknown/debug/ozjs_rust.wasm --out-dir ./dist"
  }
}
